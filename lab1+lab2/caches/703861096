 <script type="text/javascript">                        
	
   $(".post-31 .news").each(function(){
	   
	   var text = $(this).find(".tubiao").text();    //此处也可以写成 17/07/2014 一样识别 
if(text=="微信"){
      $(this).find(".news_imgs").addClass("wx"); 
    }
    if(text=="微博"){
      $(this).find(".news_imgs").addClass("wb"); 
    }
	  if(text=="抖音"){
      $(this).find(".news_imgs").addClass("dy"); 
    }
    if(text=="B站"){
      $(this).find(".news_imgs").addClass("bili"); 
    }
    if(text=="新华网"){
      $(this).find(".news_imgs").addClass("xhw"); 
    }
    if(text=="视频中心"){
      $(this).find(".news_imgs").addClass("spzx"); 
    }
    if(text=="视频号"){
      $(this).find(".news_imgs").addClass("sph"); 
    }
    if(text=="人民号"){
      $(this).find(".news_imgs").addClass("emh"); 
    }
    if(text=="哈工大报"){
      $(this).find(".news_imgs").addClass("hgdb"); 
    }
	  
   });
</script>
